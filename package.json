{
  "name": "vwc-web-app",
  "version": "1.0.0",
  "repository": "Vets-Who-Code/vwc-site",
  "dependencies": {
    "eslint": "5.6.0",
    "gatsby": "1.9.273",
    "gatsby-link": "1.6.34",
    "gatsby-plugin-create-client-paths": "1.0.1",
    "gatsby-plugin-react-helmet": "1.0.8",
    "generate": "0.14.0",
    "jest": "23.5.0",
    "react-modal": "3.4.5",
    "react-slick": "0.23.1",
    "slick-carousel": "1.8.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "view:coverage": "open coverage/lcov-report/index.html",
    "eslint": "./node_modules/eslint/bin/eslint.js ./src/**/*.js !./src/assets",
    "lint-staged": "lint-staged",
    "heroku-postbuild": "gatsby build"
  },
  "devDependencies": {
    "enzyme": "3.3.0",
    "enzyme-adapter-react-15": "1.0.6",
    "enzyme-to-json": "3.3.4",
    "husky": "1.0.0",
    "lint-staged": "7.3.0",
    "prettier": "^1.9.2",
    "react-test-renderer": "15.6.1"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run eslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "./pre-commit.sh"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./tests/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/pages/**/*.{js,jsx}",
      "!/static/vendor/jquery/dist"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/image-mock.js",
      "\\.(css|scss|sass)$": "<rootDir>/tests/__mocks__/style-mock.js"
    }
  }
}
