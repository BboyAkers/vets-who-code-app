{
  "name": "vwc-web-app",
  "version": "1.0.0",
  "repository": "Vets-Who-Code/vwc-site",
  "dependencies": {
    "babel-jest": "23.4.2",
    "babel-plugin-transform-es2015-arrow-functions": "6.22.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-15": "1.0.6",
